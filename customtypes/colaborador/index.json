{
  "format": "custom",
  "id": "colaborador",
  "label": "Colaborador",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "foto": {
        "type": "Image",
        "config": {
          "label": "Foto",
          "constraint": {
            "width": 528,
            "height": 600
          },
          "thumbnails": [
            {
              "name": "Tela estreita",
              "width": 312,
              "height": 380
            }
          ]
        }
      },
      "nome": {
        "type": "Text",
        "config": {
          "label": "Nome",
          "placeholder": "Nome e sobrenome (preferencialmente apenas 2)"
        }
      },
      "apresentacao_curta": {
        "type": "StructuredText",
        "config": {
          "label": "Apresentação curta",
          "placeholder": "Parágrafo único",
          "allowTargetBlank": true,
          "single": "paragraph,strong,em"
        }
      },
      "apresentacao_longa": {
        "type": "StructuredText",
        "config": {
          "label": "Apresentação longa",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em"
        }
      },
      "ordem": {
        "type": "Number",
        "config": {
          "label": "Ordem",
          "placeholder": "Valor maior ou igual a 1"
        }
      }
    }
  }
}
